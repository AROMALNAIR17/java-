class Solution {
    public List<Integer> findDisappearedNumbers(int[] nums) {
        List <Integer> list= new ArrayList<>();
        int n = nums.length;
        int arr[] =new int[n];
        for(int i=0;i<n;i++) {
            arr[nums[i]-1]=1;
        }     
        for(int i=0;i<n;i++ ){
           if (arr[i] == 0){
            list.add (i+1);
           }
        }  
        return list;
    }
}
