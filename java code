public class hello {
    public static void main(String[] args) {
        
        System.out.println("Using FOR loop:");
        for (int i = 1; i <= 5; i++) {
            
            System.out.println("FOR loop value: " + i);
        }
        System.out.println("\nUsing WHILE loop:");
        int j = 6;
        while (j <= 10) {
            System.out.println("WHILE loop value: " + j);
            j++;
        }
    }
}
